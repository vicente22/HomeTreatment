<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".views.result.ResultDataActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/back"
                android:padding="10dp"
                android:text="RESUMEN DE TRATAMIENTO:"
                android:textColor="@color/colorPrimaryText"
                android:textSize="16sp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Nombre: " />

            <TextView
                android:id="@+id/textViewResultName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/back"
                android:textAlignment="center"
                android:textColor="@color/colorPrimaryText"
                android:textSize="20sp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Edad: " />

            <TextView
                android:id="@+id/textViewResultAge"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/back"
                android:textAlignment="center"
                android:textColor="@color/colorPrimaryText"
                android:textSize="20sp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Malestar: " />

            <TextView
                android:id="@+id/textViewResultMyAnnoyance"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/back"
                android:textAlignment="center"
                android:textColor="@color/colorPrimaryText"
                android:textSize="20sp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Tratamiento: " />

            <TextView
                android:id="@+id/textViewHomeTreatment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/back"
                android:textAlignment="center"
                android:textColor="@color/colorPrimaryText"
                android:textSize="20sp" />

            <fragment
                tools:layout="@layout/fragment_registered"
                android:id="@+id/fragmentRegistered"
                android:name="cl.vicentepc.miappprueba2.views.register.RegisteredFragment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></fragment>

        </LinearLayout>

    </ScrollView>

</LinearLayout>